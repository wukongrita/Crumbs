            {{/bs-dropdown-toggle}}
            {{#bs-dropdown-menu classNames="dropdown-custom dropdown-menu-right"}}
              <li class="dropdown-header">Account</li>
              <li>
                {{#link-to "m.biscuit.profiles"}}<i class="fa fa-user fa-fw"></i> {{t "navbar.user.profile"}}{{/link-to}}
                {{#link-to "m.dashboard"}}<i class="fa fa-gear fa-fw"></i> {{t "navbar.settings"}}{{/link-to}}
              </li>
              <li class="divider"></li>
              <li><a href="#" {{action "logout"}}><i class="fa fa-sign-out fa-fw"></i> {{t "navbar.logout"}}</a></li>
            {{/bs-dropdown-menu}}
          {{/bs-dropdown}}
        </ul>
      </div>
    </nav>
  </header>

  <div class="wrapper row-offcanvas row-offcanvas-left">
    <!-- Left side column. contains the logo and sidebar -->
    <aside class="left-side sidebar-offcanvas {{if isSidebarCollapsed "collapse-left"}}">
      <!-- sidebar: style can be found in sidebar.less -->
      {{assets/b-sidebar elementId="side-menu"}}
      <!-- /.sidebar -->
    </aside>

    <!-- Right side column. Contains the navbar and content of the page -->
    <aside class="right-side {{if isSidebarCollapsed "strech"}}">
      <!-- Content Header (Page header) -->


      <!-- Main content -->
      <section class="content">
        {{outlet}}
      </section>
    </aside>
  </div>

{{else}}
  {{outlet}}
{{/if}}

