{{#if session.isAuthenticated}}
  <!-- header logo: style can be found in header.less -->
  <header class="header">
    {{#link-to "m.dashboard" title="Dashboard" class="logo"}}Crumbs{{/link-to}}

    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-fixed-top" role="navigation">
      <!-- Sidebar toggle button-->
      <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button" {{action "sidebarToggle"}}>
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </a>

      {{!-- <div class="navbar-right"> --}}
      <div class="navbar-right">
        <ul class="nav navbar-nav">
        {{!--
          {{#bs-dropdown tagName="li" classNames="user user-menu"}}
